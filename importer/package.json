{
	"name": "bookbrainz-import",
	"version": "0.1.0",
	"description": "Import scripts for various data sources",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "rimraf lib/* && babel src --out-dir lib --extensions .js,.ts && tsc",
		"nodemon": "nodemon --watch './src/' --exec 'npm run build'",
		"lint": "eslint ./src",
		"lint-staged": "lint-staged",
		"consumer": "node ./lib/consumer/index.js",
		"importOLWorks": "node ./lib/openLibrary/producer/index.js -d works",
		"importOLAuthors": "node ./lib/openLibrary/producer/index.js -d authors",
		"importOLEditions": "node ./lib/openLibrary/producer/index.js -d editions"
	},
	"lint-staged": {
		"*.js": [
			"eslint"
		],
		"*.ts": [
			"eslint"
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/bookbrainz/bookbrainz-import.git"
	},
	"keywords": [
		"bookbrainz",
		"book",
		"data",
		"import"
	],
	"author": "Shivam Tripathi",
	"license": "GPL-2.0+",
	"dependencies": {
		"amqplib": "^0.10.3",
		"async": "^2.6.0",
		"bookbrainz-data": "^4.0.1",
		"franc-min": "^6.1.0",
		"immutable": "^3.8.2",
		"lodash": "^4.17.21",
		"validator": "^13.9.0",
		"winston": "^3.9.0",
		"yargs": "^17.7.2"
	},
	"devDependencies": {
		"@babel/cli": "^7.14.3",
		"@babel/core": "^7.14.3",
		"@babel/preset-env": "^7.14.4",
		"@babel/preset-typescript": "^7.13.0",
		"@babel/register": "^7.13.16",
		"@types/amqplib": "^0.10.1",
		"@types/lodash": "^4.14.168",
		"@types/node": "^18.11.18",
		"@types/yargs": "^17.0.24",
		"@typescript-eslint/eslint-plugin": "^5.48.2",
		"@typescript-eslint/parser": "^5.48.2",
		"babel-plugin-lodash": "^3.3.4",
		"eslint": "^8.32.0",
		"eslint-plugin-babel": "^5.1.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-node": "^11.1.0",
		"husky": "^8.0.0",
		"lint-staged": "^13.1.0",
		"nodemon": "^2.0.4",
		"rimraf": "^2.6.2",
		"typescript": "^4.2.4"
	}
}
